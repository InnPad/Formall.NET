<style type="text/css">
.#@(Model.Prefix)surface {
    @include inline-block;
    overflow: hidden;
}

.rvml {
    behavior: url(#default#VML);
}

.#@(Model.Prefix)surface tspan {
    @include no-select;
}

.#@(Model.Prefix)vml-sprite {
    position: absolute;
    left: 0;
    top: 0;
    width: 1px;
    height: 1px;
}

.#@(Model.Prefix)vml-group {
    position: absolute;
    left: 0;
    top: 0;
    width: 1000px;
    height: 1000px;
}

.#@(Model.Prefix)vml-measure-span {
    position: absolute;
    left: -9999em;
    top: -9999em;
    padding: 0;
    margin: 0;
    display: inline;
}

.#@(Model.Prefix)vml-base {
    position: relative;
    top: 0;
    left: 0;
    overflow: hidden;
    display: inline-block;
}

.#@(Model.Prefix)vml-base {
    position: relative;
    top: 0;
    left: 0;
    overflow: hidden;
    display: inline-block;
}

svg, vml {
    overflow: hidden;
}
</style>