<style type="text/css">
.#@(Model.Prefix)form-item-label {
    display: block;
}

.#@(Model.Prefix)form-item-label-right {
    text-align: right;
}

.#@(Model.Prefix)form-item-label-top {
    display: block;
    @if (Model.IncludeIE)
    {
        <text>
        zoom: 1;
        </text>
    }
}

.#@(Model.Prefix)form-invalid-icon {
    overflow: hidden;
    ul {
        display: none;
    }
}
</style>
